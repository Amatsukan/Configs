[webtatic-testing]
name=Webtatic Repository EL7 - $basearch - Testing
#baseurl=https://repo.webtatic.com/yum/el7-testing/$basearch/
mirrorlist=https://mirror.webtatic.com/yum/el7-testing/$basearch/mirrorlist
failovermethod=priority
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-webtatic-el7

